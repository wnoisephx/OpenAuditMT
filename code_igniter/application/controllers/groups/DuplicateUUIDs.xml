<group>
	<details>
		<group_name>Duplicate UUIDs</group_name>
		<group_padded_name/>
		<group_dynamic_select><![CDATA[SELECT distinct(system.system_id) FROM system INNER JOIN (SELECT uuid FROM system GROUP BY uuid HAVING count(uuid)> 1) system2 ON system.uuid = system2.uuid WHERE system2.uuid != '']]></group_dynamic_select>
		<group_parent>1</group_parent>
		<group_description>Any items in the system table that have an identical UUID entry regardless of status.</group_description>
		<group_category>general</group_category>
		<group_display_sql><![CDATA[SELECT system.icon, system.system_id, system.hostname, system.uuid, system.man_status, system.man_type, system.man_os_family, system.man_os_name, system.timestamp FROM system, oa_group_sys WHERE system.system_id = oa_group_sys.system_id AND oa_group_sys.group_id = ? GROUP BY system.system_id]]></group_display_sql>
		<group_icon>general</group_icon>
	</details>
