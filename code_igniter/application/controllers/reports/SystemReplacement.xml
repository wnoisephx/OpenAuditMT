<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>System Replacement</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, system.man_manufacturer, system.man_model, system.man_serial, oa_location.name, oa_org.name as org_name, system.man_purchase_date FROM system LEFT JOIN oa_group_sys ON (system.system_id = oa_group_sys.system_id) LEFT JOIN oa_location ON (system.man_location_id
 = oa_location.id) LEFT JOIN oa_org ON (system.man_org_id = oa_org.id) WHERE oa_group_sys.group_id = @group]]></report_sql>
		<report_display_sql></report_display_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
