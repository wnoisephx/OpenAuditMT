<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>System Manual Export</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, system.man_vm_group, system.man_owner, system.man_description, system.man_function, system.man_environment, system.man_org_id, system.man_location_id
, oa_location.name FROM system LEFT JOIN oa_group_sys ON (system.system_id = oa_group_sys.system_id) LEFT JOIN oa_location ON (system.man_location_id
 = oa_location.id) WHERE oa_group_sys.group_id = @group GROUP BY system.hostname ORDER BY system.hostname]]></report_sql>
		<report_display_sql></report_display_sql>
		<report_view_file>v_report</report_view_file>
