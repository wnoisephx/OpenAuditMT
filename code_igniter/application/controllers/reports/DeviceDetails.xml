<report>
	<details>
		<report_name>Device Details</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, system.icon, system.man_type, date(system.first_timestamp) as first_seen, date(system.last_seen) as last_seen, system.last_seen_by, system.man_model, system.man_manufacturer, system.man_serial, system.man_owner, oa_location.name, system.os_family, oa_org.name as org_name FROM system LEFT JOIN oa_location ON system.man_location_id
 = oa_location.id LEFT JOIN oa_group_sys ON system.system_id = oa_group_sys.system_id LEFT JOIN oa_org ON system.man_org_id = oa_org.id WHERE oa_group_sys.group_id = @group GROUP BY system.system_id ORDER BY system.hostname]]></report_sql>
		<report_view_file>v_report</report_view_file>
