<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Consumed IP Addresses</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, system.man_domain, system.icon, system.man_os_family, ip.ip as man_ip_address, system.man_description FROM ip LEFT JOIN system ON (system.system_id = ip.system_id AND ip.current = 'y') LEFT JOIN oa_group_sys ON (system.system_id = oa_group_sys.system_id) WHERE group_id = @group AND ip.ip IS NOT NULL AND ip.ip != '127.000.000.001' AND ip.ip != '' AND ip.version = '4' GROUP BY ip.system_id, ip.ip ORDER BY ip.ip]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>3</report_sort_column>
		<report_description>The ip addresses used by a group.</report_description>
	</details>
