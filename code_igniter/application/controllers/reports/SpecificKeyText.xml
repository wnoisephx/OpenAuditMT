		<report_display_in_menu>n</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, software_key.name, software_key.string FROM system LEFT JOIN software_key ON (system.system_id = software_key.system_id AND software_key.current = 'y') LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.system_id) WHERE oa_group_sys.group_id = @group AND software_key.string = ?]]></report_sql>
