<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Elevated Users</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, user_group.members, user_group.name FROM user_group LEFT JOIN system ON (user_group.system_id = system.system_id AND user_group.current = 'y') LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.system_id) WHERE oa_group_sys.group_id = @group AND (user_group.name = "Administrators" OR user_group.name = "Power Users" OR user_group.name = "Remote Desktop Users" OR user_group.name = 'wheel' OR user_group.name = 'sudo') AND user_group.members > '']]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>Name, group name, group members.</report_description>
	</details>
	<columns>
		<column>
			<column_order>0</column_order>
			<column_name>System Name</column_name>
			<column_variable>hostname</column_variable>
			<column_type>link</column_type>
			<column_link>/main/system_display/</column_link>
			<column_secondary>system_id</column_secondary>
			<column_ternary></column_ternary>
			<column_align>left</column_align>
		</column>
		<column>
			<column_order>1</column_order>
			<column_name>Name</column_name>
			<column_variable>name</column_variable>
			<column_type>text</column_type>
			<column_link></column_link>
			<column_secondary></column_secondary>
			<column_ternary></column_ternary>
			<column_align>left</column_align>
		</column>
		<column>
			<column_order>2</column_order>
			<column_name>Members</column_name>
			<column_variable>members</column_variable>
			<column_type>text</column_type>
			<column_link></column_link>
			<column_secondary></column_secondary>
			<column_ternary></column_ternary>
			<column_align>left</column_align>
		</column>
	</columns>
</report>