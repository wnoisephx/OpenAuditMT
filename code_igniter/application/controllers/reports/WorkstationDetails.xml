<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Workstation Details</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, system.man_description, system.man_serial, system.timestamp, system.man_manufacturer, system.man_model, windows.user_name, system.man_ip_address, system.man_description, oa_location.name, oa_org.name as org_name, system.man_owner FROM system LEFT JOIN oa_location ON (system.man_location_id
 = oa_location.id) LEFT JOIN oa_org ON (system.man_org_id = oa_org.id) LEFT JOIN oa_group_sys ON (system.system_id = oa_group_sys.system_id) LEFT JOIN windows ON (system.system_id = windows.system_id AND windows.current = 'y') WHERE oa_group_sys.group_id = @group]]></report_sql>
		<report_display_sql></report_display_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>Device details - name, description, last audited date, owner, Windows user, location, manufacturer, model, serial.</report_description>
	</details>
