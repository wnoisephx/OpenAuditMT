		<report_display_in_menu>n</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, system.icon, system.man_type, system.man_description, system.man_os_family, system.man_os_name, system.man_os_group, system.man_ip_address FROM system LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.system_id) WHERE oa_group_sys.group_id = @group AND system.man_os_family = (SELECT man_os_family FROM system WHERE system_id = ? LIMIT 1)]]></report_sql>
