<report>
	<details>
		<report_name>Web Sites</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, system.man_os_family, system.man_environment, server_item.parent_name, server_item.name, server_item.description, server_item.status, server_item.instance, server_item.path, server_item.log_status, server_item.log_format, server_item.log_rotation, server_item.log_path FROM system LEFT JOIN server_item ON (server_item.system_id = system.system_id and server_item.current = 'y') LEFT JOIN oa_group_sys ON (oa_group_sys.system_id = system.system_id) WHERE oa_group_sys.group_id = @group AND server_item.type = 'website']]></report_sql>
		<report_view_file>v_report</report_view_file>
