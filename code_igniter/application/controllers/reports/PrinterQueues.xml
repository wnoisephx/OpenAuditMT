<report>
	<details>
		<report_name>Printer Queues</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, print_queue.manufacturer, print_queue.model, print_queue.description, print_queue.driver, print_queue.status, print_queue.port_name, print_queue.duplex, print_queue.color, print_queue.location, print_queue.type, print_queue.shared, print_queue.shared_name, print_queue.capabilities FROM print_queue LEFT JOIN system ON (system.system_id = print_queue.system_id AND print_queue.current = 'y') LEFT JOIN oa_group_sys ON (system.system_id = oa_group_sys.system_id) WHERE oa_group_sys.group_id = @group]]></report_sql>
