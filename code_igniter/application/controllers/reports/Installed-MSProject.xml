<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Installed - MS Project</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, oa_org.name as org_name, windows.user_name, software.name, software.installed_by, software.version, software.installed_on from software LEFT JOIN system ON (system.system_id = software.system_id AND software.current = 'y' AND software.name LIKE '%Microsoft%Project%') LEFT JOIN oa_org ON (system.man_org_id = oa_org.id) LEFT JOIN windows ON (system.system_id = windows.system_id AND windows.current = 'y') LEFT JOIN oa_group_sys ON (system.system_id = oa_group_sys.system_id) WHERE oa_group_sys.group_id = @group]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>Name, software name, software version where software_name like Project.</report_description>
	</details>
