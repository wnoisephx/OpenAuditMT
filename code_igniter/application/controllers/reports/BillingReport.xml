<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>Billing Report</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, oa_org.name as org_name, date_format(system.last_seen, '%Y-%m-%d') as last_seen, system.last_seen_by, system.man_manufacturer, system.man_model, system.man_serial, system.man_type, system.man_class, CASE WHEN windows.user_name > '' THEN windows.user_name ELSE system.man_owner END as 'user', oa_location.name FROM system LEFT JOIN oa_location ON system.man_location_id
 = oa_location.id LEFT JOIN windows ON (system.system_id = windows.system_id AND windows.current = 'y') LEFT JOIN oa_group_sys ON system.system_id = oa_group_sys.system_id LEFT JOIN oa_org on system.man_org_id = oa_org.id WHERE oa_group_sys.group_id = @group]]></report_sql>
		<report_display_sql></report_display_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>Name, last seen on and by, type, class, manufacturer, model, serial, user, location.</report_description>
