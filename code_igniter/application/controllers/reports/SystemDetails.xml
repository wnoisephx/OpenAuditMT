<?xml version="1.0" encoding="ISO-8859-1"?>
<report>
	<details>
		<report_name>System Details</report_name>
		<report_display_in_menu>y</report_display_in_menu>
		<report_sql><![CDATA[SELECT system.system_id, system.hostname, system.os_name, system.man_class, system.man_owner, system.man_ip_address, system.man_model, date_format(system.first_timestamp, '%Y-%m-%d') as first_seen, date_format(system.last_seen, '%Y-%m-%d') as last_seen, system.last_seen_by, system.pc_date_os_installation as installed, system.man_manufacturer, system.man_serial, system.man_form_factor, windows.client_site_name,  windows.user_name, oa_location.name, windows.active_directory_ou FROM system LEFT JOIN oa_location ON (system.man_location_id
 = oa_location.id) LEFT JOIN windows ON (system.system_id = windows.system_id AND windows.current = 'y') LEFT JOIN oa_group_sys ON (system.system_id = oa_group_sys.system_id) WHERE oa_group_sys.group_id = @group]]></report_sql>
		<report_view_file>v_report</report_view_file>
		<report_view_contents></report_view_contents>
		<report_processing></report_processing>
		<report_sort_column>0</report_sort_column>
		<report_description>Device details - name, os, class, owner, ip, model, first and last seen date, last seen by, os install date, manufacturer, serial, form factor, Windows client site name, Windows user name, location, active directory OU.</report_description>
